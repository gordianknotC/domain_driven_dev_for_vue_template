{
  "name": "web_template_for_general_vue_pj",
  "author": "curtis",
  "private": true,
  "scripts": {
    "serve": " VITE_APP_ENV=default vite",
    "develop": " VITE_APP_ENV=development vite build",
    "prod": " VITE_APP_ENV=production vite build",
    "release": " VITE_APP_ENV=release vite build",
    "prettier": "prettier --write 'src/**/*.vue' 'src/**/*.ts'",
    "test": "vitest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "install_node": "nvm install v14.18.0 && nvm alias default v14.18.0",
    "------------------------": "",
    "       打包 ＋ serve     ": "",
    "------------------------_": "",
    "serve:dev": "yarn develop && cd dist && serve -l 4008",
    "serve:rels": "yarn release && cd dist && serve -l 4009",
    "serve:prod": "yarn prod && cd dist && serve -l 4010",
    "--------------------------": "",
    " 其 他 工 具 腳 本           ": "",
    "--------------------------_": "",
    "lint": "vue-cli-service lint",
    "css": "tailwind build src/presentation/assets/styles/index.scss -o ./src/assets/styles/index.scss",
    "compress:png": "imageoptim -a src/assets/images/**/*.png src/assets/images/*.png",
    "compress:svg": "node dumpSvgImage.js && bash compressSvgImage.sh && node updateSvgImage.js",
    "-------------------------": "",
    " 安 裝 編 譯 套 件 及 設 定 ": "",
    "-------------------------_": "",
    "install:storybook": "npx storybook init",
    "install:imageOptim": "brew install --cask imageoptim && brew install imageoptim-cli",
    "install:buildEssentials": "sudo brew install node@14.18.0 && brew install yarn && yarn global add npx && yarn global add cordova-res && yarn global add npm-run-all && brew update && brew upgrade && brew cleanup -d -v && brew install cocoapods && brew install aapt && brew install --cask imageoptim && brew install imageoptim-cli"
  },
